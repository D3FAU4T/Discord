{
    "name": "discord",
    "module": "index.ts",
    "type": "module",
    "private": true,
    "scripts": {
        "start": "NODE_ENV=production node start.mjs",
        "dev": "NODE_ENV=development node start.mjs",
        "test": "npm run test:cross",
        "start:node": "NODE_ENV=production tsx --env-file=.env.production.local index.ts",
        "dev:node": "NODE_ENV=development tsx --env-file=.env.development.local index.ts",
        "start:bun": "NODE_ENV=production bun run index.ts",
        "dev:bun": "NODE_ENV=development bun run index.ts",
        "test:bun": "bun test",
        "test:node": "tsx --env-file=.env.development.local src/tests/path.test.ts",
        "test:cross": "echo 'ðŸ§ª Running cross-runtime tests...' && bun run test:bun && echo 'âœ… Bun tests passed' && npm run test:node && echo 'âœ… Node.js tests passed' && echo 'ðŸŽ‰ All cross-runtime tests passed!'"
    },
    "devDependencies": {
        "@types/bun": "^1.2.18",
        "@types/node": "^24.0.13",
        "tsx": "^4.20.3"
    },
    "peerDependencies": {
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "cheerio": "^1.1.0",
        "discord.js": "^14.21.0",
