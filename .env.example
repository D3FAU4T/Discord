# Discord Bot Environment Configuration
# Copy this file to .env and fill in your values

# Discord Bot Token (required)
# Get this from https://discord.com/developers/applications
discordToken=your_discord_bot_token_here

# MongoDB Connection URI (required)
# Example: mongodb://localhost:27017/discord
# Or MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/
mongodbURI=your_mongodb_connection_string_here

# Development/Production Environment
# Automatically set by npm scripts, but you can override here
NODE_ENV=development
